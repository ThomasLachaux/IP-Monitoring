{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": ["airbnb-base", "plugin:import/errors", "plugin:import/warnings", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "warn",
    "no-console": "error",
    "no-alert": "error"
  }
}
